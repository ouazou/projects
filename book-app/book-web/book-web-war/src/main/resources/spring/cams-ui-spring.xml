<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jee="http://www.springframework.org/schema/jee"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                  http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.5.xsd
                  http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>
    <context:component-scan base-package="com.ypg.cams.ui"/>

    <!-- Property files -->
    <import resource="module/properties.xml"/>
    <!-- Custom security config -->
    <import resource="module/security.xml"/>
    <!-- Custom security servlet -->
    <import resource="module/servlet.xml"/>
    <!-- Monitoring config-->
    <import resource="module/monitoring.xml"/>
</beans>
